@model GoodsDisasterViewModel
@{ 
    ViewData["Title"] = "Index";
}

<div class="col-md-4">
    <form as[-controller="GoodsDisaster" asp-action="Index" method="post" role="form">
        <div class="form-group">
            <div class="alert-danger" asp-validation-summary="ModelOnly"></div>
            <label asp-for="DisId"></label>
            <select asp-for="DisId"
                    class="form-control" asp-items="@(new SelectList(ViewBag.ListOfDisasters,"Value","Text"))">
            </select>
        </div>
        <div class="form-group">
            <input id="Submit" class="btn btn-success" type="submit" value="Submit" />
        </div>

    </form>
</div>